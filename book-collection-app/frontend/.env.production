# Production environment identifier
NODE_ENV=production
# Base path for production (if deploying to GitHub Pages, use repository name, e.g., '/mini-book-collection-management/')
VITE_APP_BASE_URL=/mini-book-collection-management/
# API URL for production (use actual backend API if available; otherwise, match development or leave empty)
VITE_APP_API_URL=https://your-backend-api.com/api
# Debug mode for production (disable console logs to reduce performance overhead)
VITE_APP_DEBUG_MODE=false